{
  "name": "@alshdavid/rpc",
  "version": "2.0.0",
  "author": "David Alsh",
  "license": "MIT",
  "types": "./dist/types/index.d.ts",
  "exports": {
    "./": {
      "require": "./.dist/cjs/",
      "module": "./.dist/esm/",
      "types": "./.dist/types/"
    }
  },
  "scripts": {
    "build": "npm run clean && npm run build:esm && npm run build:cjs && npm run build:types",
    "build:esm": "npx tsc -p tsconfig.esm.json",
    "build:cjs": "npx tsc -p tsconfig.cjs.json",
    "build:umd": "npx webpack --mode production",
    "build:types": "npx tsc -p tsconfig.types.json",
    "clean": "rm -r -f dist",
    "test": "npx jest ./src ./tests"
  },
  "devDependencies": {
    "@types/jest": "^27.0.2",
    "jest": "^27.3.1",
    "jest-junit": "^13.0.0",
    "rxjs": "^7.4.0",
    "ts-jest": "^27.0.7",
    "ts-loader": "^9.2.6",
    "tslint": "^6.1.0",
    "typescript": "^4.4.4",
    "webpack": "^5.61.0",
    "webpack-cli": "^4.9.1"
  },
  "dependencies": {
    "@types/lodash": "^4.14.176",
    "lodash": "^4.17.21"
  }
}
